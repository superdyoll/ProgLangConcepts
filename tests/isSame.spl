def allTrue = int lambda (int x) {
    if (x[0] == EOF) {
        1;
    } else {
        if (x[0] != 0) {
            allTrue(x[1:]);
        } else {
            0;
        }
    }
}

def isSame = int lambda (int a, int b) {
    allTrue(a == b);
}

let
(
    stream<int> input = read();
    a = input[0];
    b = input[1];
) {
    isSame(a,b);
}