def accumulator = int lambda (int a){
	accumulator-acc(a,0);
}

def  accumulator-acc  = int lambda (int a, int pointer){
	if (!endOfStream(a)){
		addAll(a[:pointer]) :: accumulator(a, pointer++);
	} else {
		addAll(a);
	}
}

let (
    stream<int> input = read();
    int a = input[0];
){
    print(accumulator(a));
}
